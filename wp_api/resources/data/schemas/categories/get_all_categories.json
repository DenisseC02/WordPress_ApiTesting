							"properties": {
								"name": {
									"$id": "#root/items/_links/curies/items/name",
									"title": "Name",
									"type": "string",
									"default": "",
									"examples": [
										"wp"
									],
									"pattern": "^.*$"
								},
								"href": {
									"$id": "#root/items/_links/curies/items/href",
									"title": "Href",
									"type": "string",
									"default": "",
									"examples": [
										"https://api.w.org/{rel}"
									],
									"pattern": "^.*$"
								},
								"templated": {
									"$id": "#root/items/_links/curies/items/templated",
									"title": "Templated",
									"type": "boolean",
									"examples": [
										true
									],
									"default": true
								}
							}
						}

					}
				}
			}

		}
