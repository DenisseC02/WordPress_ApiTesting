	"default": [],
	"items":{
		"$id": "#root/items",
		"title": "Items",
		"type": "object",
		"required": [
			"id",
			"count",
			"description",
			"link",
			"name",
			"slug",
			"taxonomy",
			"parent",
			"meta",
			"_links"
		],
		"properties": {
			"id": {
				"$id": "#root/items/id",
				"title": "Id",
				"type": "integer",
				"examples": [
					207
				],
				"default": 0
			},
			"count": {
				"$id": "#root/items/count",
				"title": "Count",
				"type": "integer",
				"examples": [
					0
				],
				"default": 0
			},
			"description": {
				"$id": "#root/items/description",
				"title": "Description",
				"type": "string",
				"default": "",
				"examples": [
					""
