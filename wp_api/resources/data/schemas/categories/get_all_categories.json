									"default": "",
									"examples": [
										"http://localhost/wordpress/wp-json/wp/v2/taxonomies/category"
									],
									"pattern": "^.*$"
								}
							}
						}

					},
					"wp:post_type": {
						"$id": "#root/items/_links/wp:post_type",
						"title": "Wp:post_type",
						"type": "array",
						"default": [],
						"items":{
							"$id": "#root/items/_links/wp:post_type/items",
							"title": "Items",
							"type": "object",
							"required": [
								"href"
							],
							"properties": {
								"href": {
									"$id": "#root/items/_links/wp:post_type/items/href",
									"title": "Href",
									"type": "string",
									"default": "",
									"examples": [
										"http://localhost/wordpress/wp-json/wp/v2/posts?categories=207"
									],
									"pattern": "^.*$"
								}
							}
						}

					},
					"curies": {
						"$id": "#root/items/_links/curies",
						"title": "Curies",
						"type": "array",
						"default": [],
						"items":{
							"$id": "#root/items/_links/curies/items",
							"title": "Items",
							"type": "object",
							"required": [
								"name",
								"href",
								"templated"
