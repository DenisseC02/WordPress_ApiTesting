				"pattern": "^.*$"
			},
			"name": {
				"$id": "#root/items/name",
				"title": "Name",
				"type": "string",
				"default": "",
				"examples": [
					"category 091238"
				],
				"pattern": "^.*$"
			},
			"slug": {
				"$id": "#root/items/slug",
				"title": "Slug",
				"type": "string",
				"default": "",
				"examples": [
					"category-091238"
				],
				"pattern": "^.*$"
			},
			"taxonomy": {
				"$id": "#root/items/taxonomy",
				"title": "Taxonomy",
				"type": "string",
				"default": "",
				"examples": [
					"category"
				],
				"pattern": "^.*$"
			},
			"parent": {
				"$id": "#root/items/parent",
				"title": "Parent",
				"type": "integer",
				"examples": [
					0
				],
				"default": 0
			},
			"meta": {
				"$id": "#root/items/meta",
				"title": "Meta",
				"type": "array",
				"default": []
